  "tokenColors": [
    {
      "settings": {
        "background": "#212121",
        "foreground": "#999999",
        "selectionBorder": "#7B7B7B",
        "findHighlight": "#F8C555",
        "findHighlightForeground": "#7EC699",
        "activeGuide": "#F08D49",
        "bracketsForeground": "#9999997F",
        "bracketsOptions": "stippled_underline",
        "bracketsContentsForeground": "#9999997F",
        "tagsOptions": "stippled_underline"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#696969",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Entity",
      "scope": "entity",
      "settings": {
        "foreground": "#f8c555"
      }
    },
    {
      "name": "Entity",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#DF5D46"
      }
    },
    {
      "name": "Tag attributes",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#F08D49"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#67cdcc"
      }
    },
    {
      "name": "Keyword Operator Assignment",
      "scope": "keyword.operator.assignment",
      "settings": {
        "foreground": "#cc99cd"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#cc99cd",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#7EC699"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "Reserved variable names",
      "scope": "variable.language",
      "settings": {
        "foreground": "#4499CD",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Function, Special Method, Block Level",
      "scope":
        "entity.name.function, keyword.other.special-method, variable.function, meta.block-level, support.function",
      "settings": {
        "foreground": "#F08D49"
      }
    },
    {
      "name": "Class, Instance, Support",
      "scope":
        "entity.name.class, entity.name.type.class, entity.name.instance, entity.name.type.instance, support.type, support.class, support.orther.namespace.use.php, meta.use.php, support.other.namespace.php, markup.changed.git_gutter",
      "settings": {
        "foreground": "#f8c555"
      }
    },
    {
      "name": "Symbols, Inherited Class",
      "scope":
        "keyword.other.template, constant.other.symbol, constant.other.key, entity.other.inherited-class, markup.heading, markup.inserted.git_gutter, meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
      "settings": {
        "foreground": "#7EC699"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope":
        "constant,constant.numeric, constant.language, support.constant, constant.character, variable.parameter, keyword.other.unit",
      "settings": {
        "foreground": "#DF5D46",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Constant Numeric",
      "scope": "constant.numeric, keyword.other.unit, support.constant",
      "settings": {
        "fontStyle": "normal"
      }
    }
  ]
