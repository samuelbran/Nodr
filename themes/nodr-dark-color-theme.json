  "tokenColors": [
    {
      "settings": {
        "background": "#2D2D2D",
        "foreground": "#888888",
        "selectionBorder": "#7B7B7B",
        "findHighlight": "#F6BF47",
        "findHighlightForeground": "#6FDF8E",
        "activeGuide": "#F08D49",
        "bracketsForeground": "#8888887F",
        "bracketsOptions": "stippled_underline",
        "bracketsContentsForeground": "#8888887F",
        "tagsOptions": "stippled_underline"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#666666",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Entity",
      "scope": "entity",
      "settings": {
        "foreground": "#F6BF47"
      }
    },
    {
      "name": "Entity",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#DF5D46"
      }
    },
    {
      "name": "Tag attributes",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#E278B4"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#888888"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#67cdcc"
      }
    },
    {
      "name": "Keyword Operator Assignment",
      "scope": "keyword.operator.assignment",
      "settings": {
        "foreground": "#A87DE0"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#E278B4"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#6FDF8E"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "foreground": "#A3A3A3"
      }
    },
    {
      "name": "Reserved variable names",
      "scope": "variable.language",
      "settings": {
        "foreground": "#4499CD",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Function, Special Method, Block Level",
      "scope": "entity.name.function, keyword.other.special-method, variable.function, meta.block-level, support.function",
      "settings": {
        "foreground": "#F08D49"
      }
    },
    {
      "name": "Class, Instance, Support",
      "scope": "entity.name.class, entity.name.type.class, entity.name.instance, entity.name.type.instance, support.type, support.class, support.orther.namespace.use.php, meta.use.php, support.other.namespace.php, markup.changed.git_gutter",
      "settings": {
        "foreground": "#F6BF47"
      }
    },
    {
      "name": "Symbols, Inherited Class",
      "scope": "keyword.other.template, constant.other.symbol, constant.other.key, entity.other.inherited-class, markup.heading, markup.inserted.git_gutter, meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
      "settings": {
        "foreground": "#6FDF8E"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": "constant,constant.numeric, constant.language, support.constant, constant.character, variable.parameter, keyword.other.unit",
      "settings": {
        "foreground": "#DF5D46",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Constant Numeric",
      "scope": "constant.numeric, keyword.other.unit, support.constant",
      "settings": {
        "fontStyle": "normal"
      }
    },
    {
      "name": "SCSS Class, Attribute Selector",
      "scope": "entity.other.attribute-name.class.css, meta.attribute-selector.scss",
      "settings": {
        "foreground": "#6FDF8E"
      }
    },
    {
      "name": "SCSS Punctuation Brackets",
      "scope": "punctuation.definition.attribute-selector.begin.bracket.square.scss, punctuation.definition.attribute-selector.end.bracket.square.scss",
      "settings": {
        "foreground": "#8a86a1"
      }
    },
    {
      "name": "SCSS Operator",
      "scope": "keyword.operator.scss",
      "settings": {
        "foreground": "#A87DE0"
      }
    },
    {
      "name": "SCSS Name Tag",
      "scope": "entity.name.tag.css",
      "settings": {
        "foreground": "#F08D49"
      }
    }
  ]
}
